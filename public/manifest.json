{
  "name": "Chrome Plugin",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "chrome集成React 开发插件",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/"
  ],
  "page_action": {
    "default": "Chrome Plugin",
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    "static/js/network.js",
    "static/js/hook.js",
    "static/js/download.js"
  ],
  "externally_connectable": {
    "matches": [
      "*://pan.baidu.com/s/*"
    ]
  },
  "background": {
    "scripts": [
      "./static/js/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://pan.baidu.com/s/*"
      ],
      "js": [
        "./static/js/insert.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://pan.baidu.com/s/*"
      ],
      "js": [
        "./static/js/insert_network.js"
      ],
      "run_at": "document_start"
    }
  ]
}